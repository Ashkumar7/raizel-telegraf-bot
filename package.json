{
  "name": "raizel-telegraf-bot",
  "version": "1.3.1",
  "description": "Telegram Bot Made Using Telegraf For Real Debrid",
  "main": "app.ts",
  "scripts": {
    "start": "pm2 start --no-daemon",
    "dev": "nodemon",
    "migration:generate": "drizzle-kit generate:pg --schema=./lib/db/schema.ts",
    "migration:migrate": "node -r esbuild-register ./lib/db/migrate.ts",
    "migration:push": "npx drizzle-kit push:pg",
    "drizzle-studio": "drizzle-kit studio"
  },
  "license": "ISC",
  "dependencies": {
    "@vercel/postgres": "^0.5.0",
    "axios": "^1.5.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.6",
    "hpagent": "^1.2.0",
    "node-os-utils": "^1.3.7",
    "pg": "^8.11.3",
    "telegraf": "^4.14.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.8.3",
    "@types/node-os-utils": "^1.3.2",
    "@types/pg": "^8.10.5",
    "drizzle-kit": "^0.19.13",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "author": "raizel-sama"
}